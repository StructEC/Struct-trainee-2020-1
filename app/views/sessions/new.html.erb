<div class="container">
  <h1 class="text-center"> Cadastro de Usu√°rios</h1>
  <%= form_with url: :login, method: :post do |form| %>

    <div class="form-group">
      <%= form.label :email %>
      <%= form.email_field :email, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= form.label :password %>
      <%= form.password_field :password, class: 'form-control' %>
    </div>

    <%= form.submit "Fazer login", class: 'btn btn-outline-primary' %>
  <% end %>
</div>